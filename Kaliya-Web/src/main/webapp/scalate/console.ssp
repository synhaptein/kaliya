<table>
    <thead>
        <tr>
            <th scope="col">Client</th>
            <th scope="col">Address</th>
        </tr>
    </thead>
    <tr class="odd">
        <td>---</td>
        <td>---</td>
    </tr>
    <tfoot>
        <tr>
            <th scope="row">Total</th>
            <td>0</td>
        </tr>
    </tfoot>
</table>
<%-- TODO print client list --%>
</div>
